{
  "name": "my-todo-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate": "graphql-codegen --config codegen.ts -r dotenv/config",
    "deploy:dev:api": "rsync -av --exclude='*.db' ./api/ vultr:/root/api",
    "deploy:dev:web": "rsync -av ./dist vultr:/var/www/",
    "deploy:dev": "npm run build -- --mode staging && npm run deploy:dev:api && npm run deploy:dev:web"
  },
  "dependencies": {
    "@apollo/client": "^3.9.6",
    "@wysimark/react": "^3.0.20",
    "apexcharts": "^3.42.0",
    "autoprefixer": "^10.4.15",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.9",
    "flowbite-react": "^0.6.4",
    "graphql": "^16.8.0",
    "js-base64": "^3.7.5",
    "postcss": "^8.4.29",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-icons": "^4.10.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.15.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.3.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/crypto-js": "^4.2.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
